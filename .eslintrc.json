{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes": ["error", "double"],
        "require-jsdoc": "off",
        "object-curly-spacing": ["error", "always"],
        "indent": ["error", 2],
        "new-cap": "off",
        "max-len": "off"
    }
}
